  <% if search_terms.length > 1 -%>
<ul class="search_terms_switcher">
    <% search_terms.each do |search_term| -%>
<li class="var_number_list">
<%= search_term -%><%= check_box_tag "search_terms[]", search_term, true,:onclick=>"clickTerm('#{search_term}')", :class=>'search_term_checkbox', :id=>search_term +"_check",:title=>"Hide or show the variables which were only found using this search term" %>
</li>
<% end -%>
</ul>
<% end -%>