
<% unless session[:cart].items.empty? %>
  <table>
    <% session[:cart].items.each do |item|%>
      <tr>
        <td><%= check_box_tag "cart_ids[]", item %></td>
        <td><%= Variable.find(item).name %></td>
        <td><%= Survey.find(Variable.find(item).survey_id).year %></td>
      </tr>
    <% end %>
  </table>
  <% else %>
  No items in cart<br/>
<% end %>
