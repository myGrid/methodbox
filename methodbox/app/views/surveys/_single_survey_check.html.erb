<% unless collection.empty? -%>
  <%#*<div width="100%">%>
  <ul class="survey">
    <% collection.each do |item| -%>
      <li><%= check_box_tag "entry_ids[]", item.id,(selected.include?(item) &&
          {:checked => 'checked'}), :class=>'survey_checkbox'  %> <%= link_to(item.title, survey_path(item)) -%></li>
    <% end -%>
    <% selected.each do |item| -%>

    <% end -%>
  </ul>
  <%#*</div>%>
<% end -%>

